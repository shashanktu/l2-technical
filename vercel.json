{
  "version": 2,
  "builds": [
    {
      "src": "app/API.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app/API.py"
    }
  ],
  "env": {
    "PYTHONPATH": "."
  },
  "functions": {
    "app/API.py": {
      "runtime": "python3.12"
    }
  }
}
